(* desc="CDSI"	*) real CDSI;
(* desc="CDDI"	*) real CDDI;


(* desc="QS"	*) real QS;
(* desc="QD"	*) real QD;
(* desc="QG"	*) real QG;

(* desc="CGG"	*) real CGG;
